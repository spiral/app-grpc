{
    "name": "spiral/app-grpc",
    "type": "project",
    "license": "MIT",
    "description": "Spiral Skeleton GRPC Application",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/app-grpc/issues",
        "source": "https://github.com/spiral/app-grpc"
    },
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spiral/framework": "^3.0",
        "spiral/queue": "^3.0",
        "spiral/roadrunner-grpc": "^2.0",
        "spiral/roadrunner-bridge": "^2.0",
        "spiral/cycle-bridge": "^2.0"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv",
            "rr get-binary"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "spiral/composer-publish-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
